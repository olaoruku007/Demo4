pipeline {
    agent any
    stages {
         stage ('Build') {
            steps {
                echo "Building with jenkins is a cool thing"
            }
        }

         stage ('Test') {
            steps {
                echo "Testing"
            }
        }
        
        stage ('Deploy') {
            steps {
                echo "Deploying"
            }
        }
    }
}
     

        
        

    
